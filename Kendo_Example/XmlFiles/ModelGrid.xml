<?xml version="1.0" encoding="utf-8" ?>
<Grid type="mainGrid" id="jsTestGrid" width="1800"
          height="600" tableId="1"
          caption="This is test table caption"
          selectCountRows="20">

  <DataSource action="GetGridData" controller_name="ComponentData" key_link="V_TEST" />

  <Update>
    <DataSource action="GridProcedure_Update" controller_name="ComponentData" key_link="TestProcedure.xml" />
  </Update>

  <Edit>
    <DataSource action="GridProcedure_Edite" controller_name="ComponentData" key_link="TestProcedure.xml" />
  </Edit>

  <Delete>
    <DataSource action="GridProcedure_Delete" controller_name="ComponentData" key_link="TestProcedure.xml" />
  </Delete>

  <COLUMNS>

    <column type="simple" name="ID" label="Id теста" width="100" hidden="false" visible="true" key="true">
      <Model editable="false" edittype="Integer" format="{0:0}">
        <default_value>0</default_value>
      </Model>
    </column>

    <!--<column type="GridLink" to_type_link="ModelGrid_Details.xml" name="name" label="Имя" width="150" hidden="false" visible="true" key="false">
      <Model editable="true" edittype="String" format="" link="v_distinct_names">
        <default_value>TestName_Default</default_value>
      </Model>
    </column>-->
    <column type="GridLink" to_type_link="ModelGrid_Details.xml" name="name" label="Имя" width="150" hidden="false" visible="true" key="false">
      <Model editable="true" edittype="String" format="" link="v_distinct_names">
        <default_value>TestName_Default</default_value>
      </Model>
    </column>

    <column type="simple" name="date" label="Дата создания" width="150" hidden="false" visible="true" key="false">
      <Model editable="true" edittype="Date" format="{0:yyyy-MM-dd HH:mm:ss}">
        <default_value></default_value>
      </Model>
    </column>

    <column type="simple" name="bool" label="логический" width="150" hidden="false" visible="true" key="false">
      <Model editable="true" edittype="Boolean" format="">
        <default_value>0</default_value>
      </Model>
    </column>

    <column type="ProcessDesignerLink" name="number" label="числовой" width="150" hidden="false" visible="true" key="false">
      <Model editable="true" edittype="Integer" format="{0:0}">
        <default_value>0</default_value>
      </Model>
    </column>

  </COLUMNS>
  <Export_Pdf file_name="Test_Export_File.pdf" caption="EXPORT TO PDF">
    <DataSource action="ExportFile" controller_name="ExportFiles" key_link="GetPdf_key" />
  </Export_Pdf>
  <Export_Excel file_name="Test_Export_File.xlsx" caption="EXPORT TO EXCEL">
    <DataSource action="ExportFile" controller_name="ExportFiles" key_link="GetExcel_key" />
  </Export_Excel>
</Grid>